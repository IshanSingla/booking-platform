{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && prisma db push   && next dev --turbo",
    "build": "prisma generate && prisma db push  && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate --no-engine",
    "db:studio": "prisma generate && prisma studio",
    "db:migrate": "prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "prisma generate && prisma db push",
    "create:superuser": "prisma generate && prisma db push   && ts-node ./src/createsuperuser.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-table": "^8.15.3",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.368.0",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "nextjs-toploader": "^1.6.11",
    "prisma": "^5.11.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "ts-node": "^10.9.2"
  }
}